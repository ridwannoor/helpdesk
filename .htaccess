<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteRule ^vendor/phpunit/phpunit/src/Util/PHP/eval-stdin.php$ - [F]
    Options +FollowSymLinks
    
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]
</IfModule>

<Files ".env">
    Order deny,allow
    Deny from all
</Files>